<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd"> 
<configuration>
    <environments default="development"> 
         <environment id="development"> 
             <transactionManager type="JDBC"/> 
             <dataSource type="POOLED">
                 <property name="driver" value="org.sqlite.JDBC"/> 
                 <property name="url" value="jdbc:sqlite:itTalent.db" />
             </dataSource> 
         </environment>
    </environments> 
<mappers>
<mapper resource="net/radzebra/itTalent/sqlmap/ConfidentLevelMapper.xml" />
<mapper resource="net/radzebra/itTalent/sqlmap/EndorsementMapper.xml" />
<mapper resource="net/radzebra/itTalent/sqlmap/TalentStoryMapper.xml" />
<mapper resource="net/radzebra/itTalent/sqlmap/ContactMapper.xml" />
<mapper resource="net/radzebra/itTalent/sqlmap/TalentMapper.xml" />
<mapper resource="net/radzebra/itTalent/sqlmap/TypeMapper.xml" />
<mapper resource="net/radzebra/itTalent/sqlmap/TalentTagMapper.xml" />
<mapper resource="net/radzebra/itTalent/sqlmap/TagMapper.xml" />



 </mappers>

</configuration>

